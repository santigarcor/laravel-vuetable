{
    "name": "santigarcor/laravel-vuetable",
    "description": "Vuetable laravel backend package",
    "keywords": ["laravel", "vuetable", "vue", "table"],
    "license": "MIT",
    "authors": [
        {
            "name": "Santiago Garcia",
            "homepage": "http://santigarcor.me"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "illuminate/database": "^5.7|^6.0|^7.0|^8.0",
        "illuminate/http": "^5.7|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.7|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^3.7|^6.0",
        "mockery/mockery": "^1.2.0"
    },
    "autoload": {
        "psr-4": {
            "Vuetable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vuetable\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Vuetable\\VuetableServiceProvider"
            ],
            "aliases": {
                "Vuetable": "Vuetable\\VuetableFacade"
            }
        }
    }
}
